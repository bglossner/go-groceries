{
  "name": "@go-groceries/frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "exports": {
    "./meals": {
      "types": "./src/shareable/meals.ts"
    },
    "./file-upload": {
      "types": "./src/shareable/file-upload.ts",
      "import": "./build/shareable/file-upload.js"
    },
    "./file-retrieval": {
      "types": "./src/shareable/file-retrieval.ts",
      "import": "./build/shareable/file-retrieval.js"
    },
    ".": {
      "types": "./src/shareable/response.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "check-types": "tsc -b",
    "build": "vite build && npm run build-node",
    "build-node": "tsc -p tsconfig.build.json",
    "build:pwa": "vite build --mode pwa",
    "build:pwa-test": "vite build --mode pwa-test",
    "check-and-build": "npm run check-types && npm run build",
    "check-and-build:pwa": "npm run check-types && npm run build:pwa",
    "check-and-build:pwa-test": "npm run check-types && npm run build:pwa-test",
    "lint": "eslint .",
    "preview": "vite preview",
    "dev:pwa": "vite --host --mode pwa-test",
    "preview:pwa": "vite build --mode pwa && vite preview --host --mode pwa",
    "preview:pwa-test": "vite build --mode pwa-test && vite preview --host --mode pwa-test",
    "upload-assets-to-s3": "node scripts/upload-to-s3.js",
    "build-and-upload:test": "npm run check-and-build:pwa-test && npm run upload-assets-to-s3 -- --bucket go-groceries-test",
    "build-and-upload:prod": "npm run check-and-build:pwa && npm run upload-assets-to-s3 -- --bucket go-groceries"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@eslint/plugin-kit": "^0.3.5",
    "@hookform/resolvers": "^5.1.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@tanstack/react-query": "^5.82.0",
    "@tanstack/react-router": "^1.125.6",
    "@tanstack/router-vite-plugin": "^1.125.6",
    "browser-image-compression": "^2.0.2",
    "dexie": "^4.0.11",
    "dexie-export-import": "^4.1.4",
    "fdir": "^6.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.60.0",
    "zod": "^4.0.2",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@tanstack/react-query-devtools": "^5.83.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@vitejs/plugin-react": "^4.6.0",
    "escape-string-regexp": "^4.0.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "tinyglobby": "^0.2.15",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.3",
    "vite-plugin-pwa": "^1.0.1",
    "yargs": "^17.7.2"
  }
}
